<package type="addon" name="nuxeo-opensocial" version="@VERSION@">
  <title>Nuxeo OpenSocial</title>
  <description>
    <p>This addon provides a user dashboard based on an OpenSocial container. This addon is deprecated.</p>
  </description>
  <home-page>http://doc.nuxeo.com/x/9Q05AQ</home-page>
  <vendor>Nuxeo</vendor>
  <installer restart="true" />
  <uninstaller restart="true" />
  <!-- <hotreload-support>false</hotreload-support> -->
  <require-terms-and-conditions-acceptance>true
  </require-terms-and-conditions-acceptance>
  <nuxeo-validation>nuxeo_certified</nuxeo-validation>
  <production-state>production_ready</production-state>
  <supported>true</supported>
  <platforms>
    <platform>cap-@DISTRIB_VERSION@</platform>
    <platform>cap-@DISTRIB_VERSION@-HF*</platform>
  </platforms>
  <license>LGPL 2.1</license>
  <license-url>http://www.gnu.org/licenses/lgpl-2.1.html</license-url>
  <visibility>PUBLIC</visibility>
</package>
